          "version": 38,
          "type": "code",
          "properties": {
            "title": [
              [
                "import { NotionRenderer } from \"react-notion\";\nimport \"react-notion/src/styles.css\";\n\nconst Page = ({ blocks, pageId }) => {\n  if (!page) return null;\n\n  return (\n    <div className=\"notion\" style={{ maxWidth: 768, color: \"rgb(55, 53, 47)\" }}>\n      <NotionRenderer\n        level={0}\n        blockMap={blocks || []}\n        currentID={pageId}\n      />\n    </div>\n  );\n};\nexport default Page;"
              ]
            ],
            "language": [["JavaScript"]]
          },
          "created_time": 1586899176842,
          "last_edited_time": 1586899680000,
          "parent_id": "60d54cdb-423d-4b11-ab87-c6cc1e1c8b98",
          "parent_table": "block",
          "alive": true,
