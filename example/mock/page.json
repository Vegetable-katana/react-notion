      "2084c953-1f25-4031-8237-dbafeb80969c": {
        "role": "reader",
        "value": {
          "id": "2084c953-1f25-4031-8237-dbafeb80969c",
          "version": 121,
          "type": "code",
          "properties": {
            "title": [
              [
                "export async function getStaticProps() {\n  const pageId = \"60d54cdb-423d-4b11-ab87-c6cc1e1c8b98\", // the Notion Page ID\n  const pageData = await agent.loadPageChunk({\n    pageId,\n    limit: 999,\n    chunkNumber: 0,\n    cursor: { stack: [] },\n    verticalColumns: false,\n  });\n\n  return {\n    props: {\n      blocks: postData.recordMap.block,\n\t\t\tpageId\n    },\n    revalidate: 10,\n  };\n}"
              ]
            ],
            "language": [["JavaScript"]]
          },
